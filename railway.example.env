# Example env for Railway deployment (do NOT commit real secrets)
# Copy this to .env or set the values in Railway project variables

# Prisma / Database
DATABASE_URL=postgresql://user:password@host:5432/dbname?schema=public

# JWT
JWT_SECRET=your_jwt_secret_here

# GitHub OAuth (if used)
GH_CLIENT_ID=your_github_client_id
GH_CLIENT_SECRET=your_github_client_secret

# Email (if used by the app)
EMAIL_USER=you@example.com
EMAIL_PASS=supersecret

# Any other envs used by api/.env
# e.g. RAILWAY_STATIC_URL, NODE_ENV, PORT
RAILWAY_STATIC_URL=
NODE_ENV=production
PORT=3000
